<infoNotaCredito>
    <fechaEmision>{{ date }}</fechaEmision>
    <dirEstablecimiento>{{ establishment.address }}</dirEstablecimiento>
    <tipoIdentificacionComprador>{{
        customer.identification_type.code
    }}</tipoIdentificacionComprador>
    <razonSocialComprador>{{ customer.name }}</razonSocialComprador>
    <identificacionComprador>{{
        customer.identification_number
    }}</identificacionComprador>
    {% if company.special_taxpayer_number %}
    <contribuyenteEspecial>{{
        company.special_taxpayer_number
    }}</contribuyenteEspecial>
    {% endif %}
    <obligadoContabilidad>{{
        company.requires_accounting ? "SI" : "NO"
    }}</obligadoContabilidad>
    <codDocModificado>{{
        referenced_document.document_type.code
    }}</codDocModificado>
    <numDocModificado>{{ referenced_document.number }}</numDocModificado>
    <fechaEmisionDocSustento>{{
        referenced_document.emission_date
    }}</fechaEmisionDocSustento>
    <totalSinImpuestos>{{ totals.subtotal }}</totalSinImpuestos>
    <valorModificacion>{{ totals.total }}</valorModificacion>
    <moneda>DOLAR</moneda>
    {% include 'Components/credit-note/taxes.xml.twig' with { 'node':
    'totalConImpuestos', 'nodeItem': 'totalImpuesto', 'taxes': totals.taxes } %}
    <motivo>{{ referenced_document.reason }}</motivo>
</infoNotaCredito>
